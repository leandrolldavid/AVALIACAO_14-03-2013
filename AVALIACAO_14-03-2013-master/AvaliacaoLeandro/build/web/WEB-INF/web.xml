<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InserirEndereco</servlet-name>
        <servlet-class>InserirEndereco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarEndereco</servlet-name>
        <servlet-class>BuscarEndereco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarEndere</servlet-name>
        <servlet-class>BuscarEndere</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisarEnderecoCriteria</servlet-name>
        <servlet-class>PesquisarEnderecoCriteria</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InserirEndereco</servlet-name>
        <url-pattern>/InserirEndereco</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarEndereco</servlet-name>
        <url-pattern>/BuscarEndereco</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarEndere</servlet-name>
        <url-pattern>/BuscarEndere</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisarEnderecoCriteria</servlet-name>
        <url-pattern>/PesquisarEnderecoCriteria</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
